<body class='body' style='padding:0 !important; margin:0 !important; display:block !important; min-width:100% !important; width:100% !important; background:#ffffff; -webkit-text-size-adjust:none;'>
	<table width='100%' cellspacing='0' cellpadding='0'>
		<tr>
			<td
				align='center' valign='top'>
				<!-- Header -->
				<table width='100%' cellspacing='0' cellpadding='0'>
					<tr>
						<td align='center'>
							<table width='650' cellspacing='0' cellpadding='0' class='mobile-shell'>
								<tr>
									<td
										class='td' style='width:650px; min-width:650px; font-size:0pt; line-height:0pt; padding:0; margin:0; font-weight:normal;'>


										<!-- SEPARATOR -->
										<table width='100%' cellspacing='0' cellpadding='0'>

											<tr>
												<td class='p30-15' style='padding: 40px 0px 40px 0px;'>
													<table width='100%' cellspacing='0' cellpadding='0'>
														<tr>
															<td
																class='text pb15' style='color:#666666; font-family:Arial, sans-serif; font-size:15px; line-height:28px; text-align:left; padding-bottom:15px;'>


																<!-- content start (contains signature image url)-->
																<h1>
																	<span style='font-family:"Arial",sans-serif;color:#666666'>{{text_your_code_is}}{{token}}
																	</span>
																</h1>
																<p style='line-height:21.0pt'>
																	<span style='font-size:11.5pt;font-family:"Arial",sans-serif;color:#666666'>
																		{{text_this_code_will_be_available_for}}
																		<br>
																		<br>
																		{{text_not_recognize}}
																		<br>
																		<br>
																	</span>
																	<a href="https://www.ifthenpay.com/" target="_blank">
																		<span style='font-size:11.5pt;font-family:"Arial",sans-serif'>
																			<img width="220" height="118" style='width:2.2916in;height:1.2291in' id="_x0000_i1025" src="http://backoffice.ifthenpay.com/Images/logotipo_cor.png">
																		</span>
																	</a>
																	<span style='font-size:11.5pt;font-family:"Arial",sans-serif;color:#666666'></span>
																</p>

																<!-- content end -->


															</td>
														</tr>
													</table>
												</td>
											</tr>
										</table>
										<!-- SEPARATOR -->


										<!-- Footer -->
										<table width='100%' cellspacing='0' cellpadding='0'>
											<tr>
												<td class='p0-15-30' style='padding-bottom: 40px;'>
													<table width='100%' cellspacing='0' cellpadding='0'>
														<tr>
															<td class='text-footer1 pb10' style='color:#777777; font-family:Arial, sans-serif; font-size:14px; line-height:20px; text-align:center; padding-bottom:10px;'>


																<p style='text-align:center;line-height:15.0pt'>
																	<span style='font-size:7.5pt;font-family:"Arial",sans-serif;color:#777777'>{{text_do_not_respond}}</span>
																</p>


															</td>
														</tr>
													</table>
												</td>
											</tr>
										</table>
										<!-- END Footer -->
									</td>
								</tr>
							</table>
						</td>
					</tr>
				</table>
				<!-- END Header -->
			</td>
		</tr>
	</table>
</body>
